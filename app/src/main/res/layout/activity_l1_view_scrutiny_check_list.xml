                        <TextView
                            android:id="@+id/tv_reject"
                            style="@style/text_data"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content" />

                    </LinearLayout>
                </androidx.cardview.widget.CardView>

                <androidx.cardview.widget.CardView
